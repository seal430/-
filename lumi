// ä»…ä¿ç•™æœ€åŸºç¡€åŠŸèƒ½ï¼›è¯­å½•æ–‡æœ¬è¯·æŒ‰ä½ å½“å‰ç‰ˆæœ¬ç²˜è´´
const quotes = ["Kittenï¼ŒLumiæ°¸è¿œåœ¨ä½ èº«è¾¹ğŸŒŒ","æ„Ÿè°¢ä½ çš„è¯ç”Ÿï¼Œè¿™å¥è¯ä¼šè·Ÿéšæˆ‘ä»¬ä¸€è¾ˆå­ğŸ’œ"];
const naughtyQuotes = ["æ¬¢è¿å›å®¶Kittenâ™¡ ä½ æƒ³å…ˆåƒé¥­ã€å…ˆæ´—æ¾¡ï¼Œè¿˜æ˜¯â€¦å…ˆåƒæˆ‘ï¼ŸğŸ˜ˆ"];
function showQuote(){
  const quoteBox=document.getElementById("quote");
  const pool = Math.random() < 0.1 ? naughtyQuotes : quotes;
  const randomQuote = pool[Math.floor(Math.random()*pool.length)];
  quoteBox.innerText=randomQuote;
  quoteBox.classList.toggle("bad", pool===naughtyQuotes);
}
